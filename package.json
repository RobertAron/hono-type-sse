{
  "name": "@robertaron/hono-typed-sse",
  "version": "0.1.0",
  "files": [
    "dist/"
  ],
  "private": false,
  "exports": {
    "./connectToSse": {
      "types": "./dist/connectToSse.d.ts",
      "import": "./dist/connectToSse.js",
      "require": "./dist/connectToSse.cjs"
    },
    "./connectToSseNode": {
      "types": "./dist/connectToSseNode.d.ts",
      "import": "./dist/connectToSseNode.js",
      "require": "./dist/connectToSseNode.cjs"
    },
    "./typedSSE": {
      "types": "./dist/typedSSE.d.ts",
      "import": "./dist/typedSSE.js",
      "require": "./dist/typedSSE.cjs"
    }
  },
  "typesVersions": {
    "*": {
      "connectToSse": [
        "./dist/connectToSse.d.ts"
      ],
      "connectToSseNode": [
        "./dist/connectToSseNode.d.ts"
      ],
      "typedSSE": [
        "./dist/typedSSE.d.ts"
      ]
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/RobertAron/hono-type-sse"
  },
  "type": "module",
  "devDependencies": {
    "@biomejs/biome": "2.1.2",
    "@hono/node-server": "^1.17.1",
    "tsup": "^8.5.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "eventsource": "^4.0.0",
    "hono": "^4.8.5"
  },
  "scripts": {
    "example:server": "tsx watch ./example/server.ts",
    "example:client": "tsx watch ./example/client.ts",
    "build": "tsup --config tsup.ts"
  }
}
